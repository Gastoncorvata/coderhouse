{
  "author": "Gaston Corvata <corvata@gmail.com>",
  "description": "Proyecto Final Coder House",
  "license": "MIT",
  "name": "scss",
  "scripts": {
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "build:css": "npm run css && npm run css-autoprefixer",
    "css": "cross-var node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js --output-style compressed scss/estilos.scss css/styles.css",
    "css-autoprefixer": "cross-var \"postcss -u autoprefixer -r css/*\""
  },
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "cross-var": "^1.1.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.4",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "eslint": "^7.9.0",
    "nodejs": "0.0.0"
  }
}
